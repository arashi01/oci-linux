# Do not edit this file.
# Place your readable configurations in /etc/profile.d/*.sh
# This file applies only to non-interactive BASH shell instances.
# For the  interactive shell configuration, see /etc/profile


# Avoid sourcing more than once
if [ -z "$SHELL_INITIALISED" ]; then

  SHELL_INITIALISED=1

  SHELL=/bin/bash

  if [ -d /etc/profile.d/ ]; then
    for script in /etc/profile.d/*.sh; do   
      [ -r "$script" ] && . "$script" > /dev/null
    done
    unset script
  fi

fi
